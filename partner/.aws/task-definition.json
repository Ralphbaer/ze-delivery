{
    "family": "partner-service",
    "executionRoleArn": "arn:aws:iam::137461123460:role/ZeDeliveryECSTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::137461123460:role/ZeDeliveryECSTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "partner-service",
           // "image": "137461123460.dkr.ecr.us-east-2.amazonaws.com/ze-delivery/partner-service:c8a2229bd4c703113dedbebefd4eb0281556c879",
            "cpu": 128,
            "memoryReservation": 512,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "command": [
                "./.bin/partner-service"
            ],
            "essential": true,
            "secrets": [
                {
					"name": "ACCESS_CONTROL_ALLOW_ORIGIN",
					"valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/access-control-allow-origin"
				},
				{
					"name": "ACCESS_CONTROL_ALLOW_HEADERS",
					"valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/access-control-allow-headers"
				},
				{
					"name": "ACCESS_CONTROL_ALLOW_METHODS",
					"valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/access-control-allow-methods"
				},
                {
					"name": "ACCESS_CONTROL_EXPOSE_HEADERS",
					"valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/access-control-expose-headers"
				},
                {
                    "name": "ENV_NAME",
                    "valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/platform/env-name"
                },
                {
                    "name": "MONGO_CONNECTION_STRING",
                    "valueFrom": "arn:aws:secretsmanager:us-east-2:137461123460:secret:/prd/partner/mongo-connectionstring"
                },
                {
                    "name": "LOG_LEVEL",
                    "valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/partner/log-level"
                },
                {
                    "name": "LOG_FORMAT",
                    "valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/partner/log-format"
                },
                {
					"name": "ADMIN_JWK_URI",
					"valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/platform/admin-jwk-uri"
                },
                {
                    "name": "SPEC_URL",
                    "valueFrom": "arn:aws:ssm:us-east-2:137461123460:parameter/prd/partner/spec-url"
				}
            ],
            "environment": [
                {
                    "name": "SERVER_ADDRESS",
                    "value": ":80"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ze-delivery/awslogs/partner",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "container-logs",
                    "awslogs-create-group": "true"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "EC2",
        "FARGATE"
    ],
    "networkMode" : "awsvpc",
    "cpu": "256",
    "memory": "1024"
}